mut <- function(t, y0, parameters){
  with(as.list(c(y0, parameters)), {
  x <- y0[1]
  y <- y0[2]
  dx <- a*x - b*x*x + c*x*y
  dy <- a*y - b*y*y + c*y*x
  list(c(dx, dy))
